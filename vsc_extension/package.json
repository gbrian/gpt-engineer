{
  "name": "gpt-engineer",
  "displayName": "GPT-ENGINEER",
  "description": "Integrate gpt-engineer with Visual Studio Code",
  "version": "0.0.1",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:gpt-engineer.runTask",
    "onCommand:gpt-engineer.improveTask",
    "onCommand:gpt-engineer.newTask"
  ],
  "dependencies": {
    "vue": "^3.4.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "myExtension.showTabs",
        "title": "Show Tabs"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "myExtensionTabs",
          "title": "gpt-engineer assistant",
          "icon": "media/artificial-intelligence-ai-icon.svg"
        }
      ]
    },
    "views": {
      "myExtensionTabs": [
        {
          "id": "tab1",
          "name": "Tab 1"
        },
        {
          "id": "tab2",
          "name": "Tab 2"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^4.0.0",
    "vscode": "^1.1.37",
    "eslint": "^7.0.0"
  }
}